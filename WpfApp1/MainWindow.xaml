<Window x:Class="WpfApp1.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfApp1"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="54*"/>
            <RowDefinition Height="339*"/>
            <RowDefinition Height="26*"/>
        </Grid.RowDefinitions>
        <StackPanel Grid.Row="0" Orientation="Horizontal" VerticalAlignment="Center" >
            <Image Source="Resources/logo.png" Margin="10" HorizontalAlignment="Left"></Image>
            <TextBlock Margin="200 0 150 0" Text="Заголовок"  FontSize="12" FontWeight="Bold" HorizontalAlignment="Center" VerticalAlignment="Center" FontFamily="Comic Sans MS" Foreground="#FF37474F"></TextBlock>
        </StackPanel>
        <StackPanel>
            
        </StackPanel>
        <Frame NavigationUIVisibility="Hidden" ContentRendered="MainFrame_ContentRendered" Grid.Row="1" Name="MainFrame"></Frame>
        <Button Content="Назад" Name ="BtnBack" Width="175" Height="30" HorizontalAlignment="Right" Margin="10" Click="BtnBack_Click" Background="#FF04A0FF" Foreground="#FF37474F"></Button>

    </Grid>
</Window>
